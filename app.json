{
  "expo": {
    "name": "infoPoint",
    "slug": "infoPoint",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#e1e4e8",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e1e4e8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mtdev.infoPoint"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#e1e4e8"
      },
      "package": "com.mtdev.infoPoint"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "a basic info point application with login"
  }
}
